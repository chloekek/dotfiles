gtk-toolbar-style=3

gtk-theme-name="Vertex-Dark"
gtk-icon-theme-name=breeze
gtk-cursor-theme-name=breeze_cursors

# Cursor size must ALSO be specified in Xresources.
gtk-cursor-theme-size=96

