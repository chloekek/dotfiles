{
    "[restructuredtext]": {"editor.tabSize": 3},
    "diffEditor.hideUnchangedRegions.enabled": true,
    "diffEditor.renderSideBySide": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.bracketPairColorization.enabled": false,
    "editor.fontFamily": "'Terminus (TTF)'",
    "editor.fontSize": 14,
    "editor.lineHeight": 22,
    "editor.minimap.renderCharacters": false,
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "on",
        "strings": "on"
    },
    "editor.rulers": [80],
    "editor.snippetSuggestions": "none",
    "editor.stickyScroll.enabled": false,
    "editor.tokenColorCustomizations": {
        "[RailsCasts]": {"comments": {"fontStyle": ""}},
        "[Solarized Dark]": {"comments": {"fontStyle": ""}},
        "[Solarized Light]": {"comments": {"fontStyle": ""}},
    },
    "editor.useTabStops": true,
    "editor.wordBasedSuggestions": "allDocuments",
    "explorer.compactFolders": false,
    "explorer.openEditors.visible": 1,
    "extensions.ignoreRecommendations": true,
    "files.associations": {"*.pyx": "python"},
    "files.autoSave": "off",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.scrollback": 10000,
    "update.mode": "none",
    "vim.ignorecase": false,
    "vim.joinspaces": false,
    "vim.textwidth": 79,
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "hidden",
    "workbench.colorCustomizations": {
        "[RailsCasts]": {"statusBar.background": "#333333"},
    },
    "workbench.colorTheme": "RailsCasts",
    "workbench.editor.enablePreview": false,
    "workbench.iconTheme": null,
    "workbench.panel.defaultLocation": "right",
    "workbench.settings.editor": "json",
    "workbench.startupEditor": "none",
    "workbench.tree.enableStickyScroll": false,
}
