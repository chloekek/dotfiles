{
    "[restructuredtext]": {"editor.tabSize": 3},
    "comment-divider.languagesMap": {"cmake": ["#", "#"]},
    "debug.javascript.codelens.npmScripts": "never",
    "diffEditor.hideUnchangedRegions.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.bracketPairColorization.enabled": false,
    "editor.fontFamily": "'Terminus (TTF)'",
    "editor.fontSize": 14,
    "editor.lineHeight": 22,
    "editor.minimap.renderCharacters": false,
    "editor.rulers": [80],
    "editor.snippetSuggestions": "none",
    "editor.useTabStops": true,
    "editor.wordBasedSuggestions": "allDocuments",
    "emmet.showExpandedAbbreviation": "never",
    "explorer.compactFolders": false,
    "explorer.openEditors.visible": 1,
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,
    "files.associations": {"*.slang": "hlsl"},
    "files.autoSave": "off",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "html.autoClosingTags": false,
    "html.autoCreateQuotes": false,
    "html.validate.scripts": false,
    "javascript.autoClosingTags": false,
    "javascript.suggest.includeAutomaticOptionalChainCompletions": true,
    "javascript.updateImportsOnFileMove.enabled": "never",
    "php.validate.enable": false,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.scrollback": 10000,
    "typescript.autoClosingTags": false,
    "typescript.updateImportsOnFileMove.enabled": "never",
    "update.mode": "none",
    "vim.ignorecase": false,
    "vim.joinspaces": false,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["g", "H"],
            "commands": [
                {"command": "editor.action.showDefinitionPreviewHover"},
            ],
        },
    ],
    "vim.textwidth": 79,
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "hidden",
    "workbench.colorCustomizations": {
        "[Monochrome Light Subtle (fork by dkamyshov)]": {
            "gitDecoration.untrackedResourceForeground": "#00CA00",
            "tab.activeBackground": "#DBDEE1",
        },
        "[Monochrome Dark (fork by dkamyshov)]": {
            "tab.activeBackground": "#272727",
        },
    },
    "workbench.colorTheme": "Monochrome Dark (fork by dkamyshov)",
    "workbench.editor.empty.hint": "hidden",
    "workbench.editor.enablePreview": false,
    "workbench.iconTheme": null,
    "workbench.panel.defaultLocation": "right",
    "workbench.settings.editor": "json",
    "workbench.startupEditor": "newUntitledFile",
}
